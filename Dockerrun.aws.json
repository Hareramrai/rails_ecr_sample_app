{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "web",
        "image": "public.ecr.aws/e7y4l1f4/rails_ecr_sample_web:latest",
        "hostname": "web",
        "essential": true,
        "portMappings": [
            {
              "hostPort": 80,
              "containerPort": 80
            }
        ],
        "links": ["app"],
        "memory": 128
      },
      {
        "name": "app",
        "image": "public.ecr.aws/e7y4l1f4/rails_ecr_sample_app:latest",
        "hostname": "app",
        "essential": false,
        "memory": 128
      }
    ]
  }