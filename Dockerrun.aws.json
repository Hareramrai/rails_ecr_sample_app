{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "app",
        "image": "hareramrai/rails_ecr_sample_app",
        "hostname": "app",
        "essential": true,
        "memory": 750,
        "Update": "true"
      },
      {
        "name": "web",
        "image": "hareramrai/rails_ecr_sample_web",
        "hostname": "web",
        "essential": false,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          }
        ],
        "links": ["app"],
        "memory": 128,
        "Update": "true"
      }
    ]
  }